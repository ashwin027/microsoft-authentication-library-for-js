{
  "compileOnSave": true,
  "compilerOptions": {
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "target": "es5",
    "module": "amd",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "out",
    "lib": [
            "dom",
            "es2015",
            "es5",
            "es6"
        ]
  }
}